include <stdio.h>
int main()
{
   int x , y , z , n , p ;
   printf("Input width: ");
   scanf("%d", &x);
   printf("Input length: ");
   scanf("%d", &y);
   printf("Input high: ");
   scanf("%d", &z);
   n = x*y*z;
   if (n>3500){
       p = n*15;
       printf("Total : %d\n", p);
   }
   else{
       p = n*12;
       printf("Total : %d\n", p);
   }
}

